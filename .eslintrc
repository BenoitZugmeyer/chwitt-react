{
    "parser": "babel-eslint",
    "env": {
        "node": true
    },
    "rules": {
        "quotes": "single",
        "no-comma-dangle": false,
        "no-underscore-dangle": false,
        "curly": [2, "multi-line"],
        "no-use-before-define": false,
        "no-shadow": false,
        "camelcase": false
    },
    "globals": {
        "React": true,
    },
}

